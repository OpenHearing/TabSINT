{
  "title": "Purdue Protocol Shakedown Testing",
  "subtitle": "ARDC Survey, Manual Audiometry, Calibration, Swept DPOAE.",
  "instructionText": "Choose the exam to perform, then view results.",
  "hideProgressBar": true,
  "navMenu":[
    {
      "text": "Back to Main Menu",
      "target": {
        "reference": "MainMenu"
      },
      "returnHereAfterward": false
    }
  ],
  "pages": [
    {
      "protocolId":"MainMenu",
      "title":"Main Menu",
      "pages":[
        {
          "id":"choose",
          "title":"Response Area Testing",
          "questionMainText":"Please select a response area to demonstrate:",
          "responseArea":{
            "type":"multipleChoiceResponseArea",
            "choices":[
              {
                "id":"Subject Information Sheet"
              },
              {
                "id": "ARDC Survey"
              },
              {
                "id":"Manual Audiometry"
              },
              {
                "id": "Calibration" 
              },
              {
                "id": "Swept DPOAE" 
              },
              {
                "id":"End All"
              }
            ]
          }, 
          "followOns":[
            {
              "conditional":"result.response=='Subject Information Sheet'",
              "target":{
                "reference":"Subject_Information_Sheet"
              } 
            },
            {
              "conditional":"result.response=='ARDC Survey'",
              "target":{
                "reference":"ARDC_Survey"
              } 
            },
            {
              "conditional":"result.response=='Manual Audiometry'",
              "target":{
                "reference":"ManualAudiometry"
              } 
            },
            {
              "conditional": "result.response=='Calibration Exam'",
              "target": {
                "reference": "CalibrationExam"
              }
            },
            {
              "conditional": "result.response=='Swept DPOAE'",
              "target": {
                "reference": "SweptDpoae"
              }
            },
            {
              "conditional":"result.response=='End All'",
              "target":{
                "reference":"@END_ALL"
              } 
            }
          ]
        }
      ]
    }
  ],
  "subProtocols":[
    {
      "protocolId": "Subject_Information_Sheet",
      "pages":[
        {
          "id": "Subject_Information_Sheet_1",
          "title": "Subject Information Sheet 1",
          "instructionText": "To be completed by researcher:",
          "responseArea": {
            "type": "multipleInputResponseArea",
            "inputList": [
              {
                "text": "Subject ID",
                "inputType": "text",
                "required": true
              },
              {
                "text": "Researcher",
                "inputType": "text"
              },
              {
                "text": "Today's Date",
                "inputType": "date",
                "dateProperties": {
                  "default": "today"
                }
              },
              {
                "text": "Name of Study",
                "inputType": "text"
              }
            ]
          }
        },
        {
          "id": "Subject_Information_Sheet_2",
          "title": "Subject Information Sheet 2",
          "instructionText": "To be completed by participant:",
          "responseArea": {
            "type": "multipleInputResponseArea",
            "inputList": [
              {
                "text": "Birhtdate",
                "inputType": "date",
                "dateProperties": {
                  "maxDate": "today"
                }
              },
              {
                "text": "Age",
                "inputType": "number"
              },
              {
                "text": "Gender",
                "inputType": "dropdown",
                "options": [
                  "Male",
                  "Female",
                  "Non-binary",
                  "Not reported"
                ]
              },
              {
                "text": "Are you a native speaker of American English?",
                "inputType": "dropdown",
                "options": [
                  "Yes",
                  "No"
                ]
              },
              {
                "text": "Are you a native speaker of American English?",
                "inputType": "dropdown",
                "options": [
                  "Yes",
                  "No"
                ]
              },
              {
                "text": "Do you speak other languages other than English?",
                "inputType": "dropdown",
                "options": [
                  "Yes",
                  "No"
                ]
              },
              {
                "text": "Are you on an F1 or J1 Visa?",
                "inputType": "dropdown",
                "options": [
                  "Yes",
                  "No"
                ],
                "notes": "**If yes, in order to be paid you will need to provide a copy of: I-94/ I/94W card, Visa, Passport, Tax Summary Report (from Glacier System), Form I- 20 (F1 visa only), For DS2019 (J1 Visa only)"
              },
              {
                "text": "Do you have a hearing loss?",
                "inputType": "dropdown",
                "options": [
                  "Yes, I have a diagnosed hearing loss",
                  "Maybe, I or others suspect I have a hearing loss, but I have not been tested",
                  "No, I do not have trouble hearing"
                ]
              },
              {
                "text": "Do you consider yourself regularly exposed to noise (e.g., play in a band, have a job with noisy tools/machines)?",
                "inputType": "dropdown",
                "options": [
                  "Yes",
                  "No"
                ]
              },
              {
                "text": "Do you have frequent ringing in your hears (tinnitus)?",
                "inputType": "dropdown",
                "options": [
                  "Yes",
                  "No"
                ]
              },
              {
                "text": "Where did you learn about our study?",
                "inputType": "text"
              }
            ]
          }
        },
        {
          "id": "Subject_Information_Sheet_3",
          "title": "Ethnicity Data",
          "instructionText": "Please fill in the following so that we can report to federal funding agencies (e.g., National Institutes of Health) the percentage of our subjects in each of their categories. NOTE: The information requested below is optional. You are not obligated to provide this information.",
          "responseArea": {
            "type": "multipleInputResponseArea",
            "inputList": [
              {
                "text": "Part A: Check the category that describes you best:",
                "inputType": "dropdown",
                "options": [
                  "Hispanic or Latino",
                  "Not Hispanic or Latino",
                  "Not Reported"
                ]
              },
              {
                "text": "Part B: Please check all the categories that apply:",
                "inputType": "multi-dropdown",
                "options": [
                  "American Indian/Alaskan native ",
                  "Asian",
                  "Native Hawaiian or Other Pacific Islander",
                  "Black or African American",
                  "White",
                  "Other",
                  "Not reported"
                ]
              }
            ]
          }
        },
        {
          "id": "backtomain",
          "reference":"MainMenu"
        }
      ]
    },
    {
      "protocolId": "ARDC_Survey",
      "pages": [
        {
          "id": "likert",
          "responseArea": {
            "type": "likertResponseArea",
            "levels": 5,
            "useEmoticons": true,
            "labels": ["Not at all", "Perfectly"],
            "questions": [
              "You are in a group of about five people, sitting round a table. It is an otherwise quiet place. You can see everyone else in the group. Can you follow the conversation? ",
              "You are talking with one other person. There is continuous background noise, such as a fan or running water close by. Can you follow what the person says? ",
              "You are in a group of about five people in a busy restaurant. You CANNOT see everyone else in the group. Can you follow the conversation? ",
              "You are talking to someone in a place where there are a lot of echoes, such as in a church or railway station. Can you follow what the other person says? "
            ]
          }
        },
        {
          "id": "ARDC_",
          "title": "ARDC 2",
          "instructionText": "To be completed by participant:",
          "responseArea": {
            "type": "multipleInputResponseArea",
            "inputList": [
              {
                "text": "Have you ever had tubes put in your eardrum(s)?",
                "inputType": "text",
                "required": true
              },
              {
                "text": "How many ear tubes?",
                "inputType": "number"
              },
              {
                "text": "Have you ever had a ruptured eardrum? ",
                "inputType": "dropdown",
                "options": [
                  "yes",
                  "no"
                ]
              },
              {
                "text": "Patient Date of Birth",
                "inputType": "date",
                "required": false,
                "dateProperties": {
                  "maxDate": "today"
                }
              },
              {
                "inputType": "multi-dropdown",
                "options": ["a","b","c"],
                "text": "Multi-dropdown Input"
              }
            ]
          }
        },
        {
          "id": "backtomain",
          "reference":"MainMenu"
        }
      ]
    },
    {
      "protocolId": "ManualAudiometry",
      "pages":[
        {
          "id": "audiometry_001",
          "title": "Manual Audiometry Test",
          "instructionText": "Test manual audiometry response area",
          "responseArea": {
            "type": "manualAudiometryResponseArea",
            "tabsintId": "1",
            "maxOutputLevel": 110,
            "minOutputLevel": -10,
            "targetLevel": 50,
            "levelUnits": "dB HL",
            "frequencies": [500, 1000, 2000, 4000, 8000],
            "adjustments": [15, -18, 5, 3],
            "retspls": {
              "500": 11.0,
              "1000": 5.5,
              "2000": 4.5,
              "4000": 9.5,
              "8000": 17.5
            }
          }
        },
        {
          "id": "backtomain",
          "reference":"MainMenu"
        }
      ]
    }, 
    {
      "protocolId": "CalibrationExam",
      "pages": [
        {
          "id": "calibration_001",
          "title": "Calibration Exam",
          "instructionText": "Test calibration response area",
          "responseArea": {
            "type": "calibrationResponseArea",
            "frequencies": [500, 1000, 2000, 4000, 8000],
            "targetLevels": [60, 70, 80, 60, 60],
            "showResults": true
          }
        },
        {
          "id": "backtomain",
