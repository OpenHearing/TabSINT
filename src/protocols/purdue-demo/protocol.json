        }
      ]
    },
    {
      "protocolId": "SweptDpoae",
      "pages": [
        {
          "id": "swept_oae_001",
          "title": "Swept DPOAE",
          "responseArea": {
            "type": "sweptDPOAEResponseArea"
          }
        },
        {
          "id": "backtomain",
          "reference": "MainMenu"
        }
      ]
    }
  ]
