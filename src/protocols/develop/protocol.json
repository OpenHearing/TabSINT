{
  "title": "Development Protocol",
  "subtitle": "Developer testing of open-tabsint responseArea's.",
  "instructionText": "instructionText goes here",
  "hideProgressBar": true,
  "navMenu":[
    {
      "text": "Back to Main Menu",
      "target": {
        "reference": "MainMenu"
      },
      "returnHereAfterward": false
    }
  ],
  "pages": [
    {
      "protocolId":"MainMenu",
      "title":"Main Menu",
      "pages":[
        {
          "id":"choose",
          "title":"Response Area Testing",
          "questionMainText":"Please select a response area to test:",
          "responseArea":{
            "type":"multipleChoiceResponseArea",
            "choices":[
              {
                "id":"Text Box"
              },
              {
                "id":"External"
              },
              {
                "id":"End All"
              }
            ]
          }, 
          "followOns":[
            {
              "conditional":"result.response=='Text Box'",
              "target":{
                "reference":"TextBox"
              } 
            },
            {
              "conditional":"result.response=='External'",
              "target":{
                "reference":"External"
              } 
            },
            {
              "conditional":"result.response=='End All'",
              "target":{
                "reference":"@END_ALL"
              } 
            }
          ]
        }
      ]
    }
  ],
  "subProtocols":[
    {
      "protocolId": "TextBox",
      "pages":[
        {
          "id": "textbox_001",
          "title": "Text Box",
          "instructionText": "Test Case 001",
          "responseArea": {
            "type": "textboxResponseArea",
            "rows": 5,
            "responseRequired": false
          }
        },
        {
          "id": "textbox_002",
          "title": "Text Box",
          "instructionText": "Test Cases 002",
          "responseArea": {
            "type": "textboxResponseArea",
            "exportToCSV": true,
            "rows": 3
          }
        },
        {
          "id": "textbox_003",
          "title": "Text Box",
          "instructionText": "Test Cases 003",
          "responseArea": {
            "type": "textboxResponseArea",
            "exportToCSV": true
          }
        },
        {
          "id": "backtomain",
          "reference":"MainMenu"
        }
      ]
    },
    {
      "protocolId": "External",
      "pages":[
        {
          "id": "external_001",
          "title": "External Test",
          "instructionText": "Test Case 001",
          "responseArea": {
            "type": "externalResponseArea",
            "subtype": "customTextboxResponseArea",
            "externalHTML": "<div><textarea id='testID' autofocus='true' maxlength='5000'></textarea></div>",
            "externalJS": "window.numRows=4;console.log('numRows:',window.numRows);let textarea=document.getElementById('testID');textarea.rows=window.numRows;htmlEle.addEventListener('input',event=>{console.log('event.target.value',event.target.value);});"
          }
        },
        {
          "id": "backtomain",
          "reference":"MainMenu"
        }
      ]
    }
  ]
}