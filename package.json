{
  "name": "tabsint",
  "version": "5.0.0",
  "scripts": {
    "ng": "ng",
    "clean": "rm -rf dist && rm -rf .angular/cache",
    "start": "ng serve",
    "prepare": "ts-node ./bin/write-version.ts",
    "build": "npm run prepare && ng build",
    "build:plugin": "cd tabsintfs && rm -rf node_modules && npm i && npm run build && cd .. && npm i ./tabsintfs",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage",
    "test:headless": "ng test --code-coverage --browsers=ChromeHeadlessNoSandbox --watch=false",
    "run.build": "npm run build && npx cap sync android && cd android && ./gradlew assembleDebug && ./gradlew installDebug && cd ..",
    "run.build.beta:ci": "npm run build --yes && npx cap sync android && cd android && ./gradlew assembleDebug && cd ..",
    "run.build.production:ci": "npm run build -- --configuration production && npx cap sync android && cd android && ./gradlew assembleRelease && cd ..",
    "run.android": "npm run build && npx cap sync android && npx cap run android",
    "serve:ssr:tabsint": "node dist/tabsint/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/cdk": "^17.1.0",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/core": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/material": "^17.2.0",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/platform-server": "^17.1.0",
    "@angular/router": "^17.1.0",
    "@angular/ssr": "^17.1.1",
    "@capacitor-community/bluetooth-le": "^6.0.0",
    "@capacitor-community/sqlite": "^6.0.1",
    "@capacitor/android": "^6.1.0",
    "@capacitor/angular": "^2.0.3",
    "@capacitor/core": "^6.1.0",
    "@capacitor/device": "^6.0.0",
    "@capacitor/filesystem": "^6.0.0",
    "@capacitor/ios": "^6.1.0",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@popperjs/core": "^2.11.8",
    "ajv": "^8.17.1",
    "angular-material-icons": "^0.7.1",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.3",
    "csv-parser": "^3.1.0",
    "d3": "^7.9.0",
    "express": "^4.18.2",
    "ionicons": "^2.0.1",
    "jeep-sqlite": "^2.7.2",
    "jsonformatter": "^0.7.0",
    "lodash": "^4.17.21",
    "ngx-json-viewer": "^3.2.1",
    "rxjs": "~7.8.0",
    "simple-git": "^3.27.0",
    "tabsintfs": "file:tabsintfs",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.1",
    "@angular/cli": "^17.1.1",
    "@angular/compiler-cli": "^17.1.0",
    "@angular/localize": "^17.2.1",
    "@capacitor/cli": "^6.1.0",
    "@types/d3": "^7.4.3",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.2"
  }
}
